  "keywords": [],
  "scripts": {
    "build": "rm -f ./cache/solidity-files-cache.json && npx hardhat run --no-compile scripts/build.ts",
    "clean": "          npx hardhat clean && rm -rf ./.openzeppelin ./cache ./typechain",
    "deploy:dev": "     npx hardhat --network localhost   deploy --no-compile --export \"contracts-1337.json\"",
    "deploy:stage": "   npx hardhat --network stage       deploy --no-compile --export \"contracts-1338.json\"",
    "deploy:testnet": " npx hardhat --network goerli      deploy --no-compile --export \"contracts-5.json\"",
    "dev": "            npx hardhat --network hardhat     node --hostname 0.0.0.0 --port 8545 2>&1 | sed -e \"s|cache/solpp-generated-\\(contracts/.*\\)|\\x1b[31m$(pwd)/\\1\\x1b[0m|g\" ",
    "dev:watch": "      npx hardhat --network hardhat     node --hostname 0.0.0.0 --port 8545 --watch 2>&1 | sed -e \"s|cache/solpp-generated-\\(contracts/.*\\)|\\x1b[31m$(pwd)/\\1\\x1b[0m|g\" ",
    "setup:dev": "      npx hardhat --network localhost   run --no-compile ./scripts/setup.ts",
    "start": "          npx hardhat --network hardhat     node --hostname 0.0.0.0 --port 8545 --no-deploy --no-impersonation",
    "start:dev": "      npx hardhat --network hardhat     node --hostname 0.0.0.0 --port 8545 --no-deploy",
    "test": "           npx hardhat test --bail ./test/test.test.ts ",
    "test:watch": "     nodemon --delay 10 -w './test' -w './contracts' -e 'sol, ts' -x 'yarn test'"
  },
  "private": true
}
